        <script type="text/javascript" language="javascript">
        
        function determineFactors(inputElement) {
                
                var value = inputElement.value;
              
                value = parseInt(value);
               
                var factors = [];
                
                for(var i = 1; i <= value; i++) {
                    
                        if(value % i == 0) {
                                factors.push(i);
                        }
                }
                
                document.getElementById("result").innerHTML = "Factors: " + factors;
        }
        </script>
</head>

<body>
    <center>
        <form name="myForm" action="javascript:void(0)" onsubmit="determineFactors(num);">
                <input type="text" name="num" />
                <input type="submit" value="Find factors"> </form>
        <p id="result"></p>
</body>

